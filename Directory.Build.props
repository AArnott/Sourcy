<Project>
    <ItemGroup Condition="'$(MSBuildProjectName)' != 'Sourcy'">
        <ProjectReference
            Include="$(MSBuildThisFileDirectory)\Sourcy\Sourcy.csproj"
            OutputItemType="Analyzer" ReferenceOutputAssembly="true"
            PrivateAssets="all" />
    </ItemGroup>

    <PropertyGroup Condition="'$(DebugSourcy)' == 'true'">
        <DefineConstants>$(DefineConstants);DEBUG_SOURCY</DefineConstants>
    </PropertyGroup>
</Project>